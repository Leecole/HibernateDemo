<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.model.Items" table="items" schema="test">
        <id name="itemId">
            <column name="itemId" sql-type="int(11)"/>
        </id>
        <property name="count">
            <column name="count" sql-type="int(11)"/>
        </property>
        <property name="proId">
            <column name="proId" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>